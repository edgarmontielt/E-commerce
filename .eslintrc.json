{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "indent": ["error", 2],
    "linebreak-style": ["error", "unix"],
    "quotes": ["error", "double"],
    "react/prop-types": "off",
    "react/react-in-jsx-scope": 0,
    "react/jsx-uses-react": 0,
    "semi": "warn",
    "react/jsx-key": "off"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
